<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ei.mapper.TenseMapper">
    <select id="getAllTense" resultType="Tense">
        select * from tenses
    </select>

    <insert id="insertTense" parameterType="Tense">
        insert into tenses (id, tenseName)
        values (#{id}, #{tenseName})
    </insert>
</mapper>
